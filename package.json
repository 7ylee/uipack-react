{
  "name": "uipack-react",
  "version": "1.0.15",
  "description": "UI Component Library",
  "main": "./dist/uipack-react.js",
  "scripts": {
    "lint": "eslint src",
    "start": "parcel src/index.html --open --out-dir dev",
    "build": "npm run lint && npm test && parcel build src/uipack-react.js --no-source-maps",
    "docs": "eslint src && parcel build src/index.html --out-dir docs --public-url '/uipack-react/'",
    "deploy": "npm run build && npm publish",
    "test": "jest"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/test-setup.js"
  },
  "keywords": [
    "React",
    "UI Framework"
  ],
  "author": "taeyouklee@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "parcel": "^1.10.3",
    "prettier": "^1.15.2",
    "react-test-renderer": "^16.6.3"
  },
  "dependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "styled-components": "^4.1.1"
  }
}
