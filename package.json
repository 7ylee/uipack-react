{
  "name": "uipack-react",
  "version": "1.0.15",
  "description": "UI Component Library",
  "main": "lib/uipack-react.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run build:demo",
    "dev": "webpack --progress --colors --watch --env dev",
    "lint": "eslint src",
    "deploy": "npm run build && npm publish",
    "test": "jest --updateSnapshot",
    "dev:demo": "webpack-dev-server --config webpack.dev.js",
    "build:demo": "webpack --env build --config webpack.dev.js"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/test-setup.js"
  },
  "keywords": [
    "React",
    "styled-components",
    "UI Framework"
  ],
  "author": "taeyouklee@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-styled-components": "^1.10.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.5.0",
    "prettier": "^1.16.4",
    "react-test-renderer": "^16.8.4",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "yargs": "^13.2.2"
  },
  "dependencies": {
    "color": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "styled-components": "^4.1.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/react"
    ],
    "plugins": [
      "babel-plugin-add-module-exports",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-class-properties",
      [
        "styled-components",
        {
          "ssr": true,
          "minify": true,
          "pure": true
        }
      ]
    ]
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "IE >= 9"
  ]
}
