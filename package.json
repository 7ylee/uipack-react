{
  "name": "uipack-react",
  "version": "1.0.15",
  "description": "UI Component Library",
  "main": "./dist/uipack-react.js",
  "scripts": {
    "lint": "eslint src",
    "start": "parcel src/index.html --open --out-dir dev",
    "build": "npm run lint && npm test && parcel build src/uipack-react.js --no-source-maps",
    "docs": "npm run lint && npm test && parcel build src/index.html --out-dir docs --public-url '/uipack-react/' --no-source-maps",
    "deploy": "npm run build && npm publish",
    "test": "jest --updateSnapshot"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/test-setup.js"
  },
  "keywords": [
    "React",
    "UI Framework"
  ],
  "author": "taeyouklee@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "babel-jest": "^24.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "jest": "^24.1.0",
    "parcel": "^1.10.3",
    "prettier": "^1.15.2",
    "react-test-renderer": "^16.6.3"
  },
  "dependencies": {
    "color": "^3.1.0",
    "modern-normalize": "^0.5.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-on-screen": "^2.1.1",
    "styled-components": "^4.1.1"
  }
}
